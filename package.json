{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "prepare": "husky install"
    },
    "dependencies": {
        "next": "12.1.4",
        "react": "18.0.0",
        "react-dom": "18.0.0"
    },
    "devDependencies": {
        "@types/node": "17.0.23",
        "@types/react": "17.0.43",
        "@types/react-dom": "17.0.14",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "8.12.0",
        "eslint-config-next": "12.1.4",
        "eslint-config-prettier": "^8.5.0",
        "husky": ">=6",
        "lint-staged": ">=10",
        "prettier": "2.6.2",
        "typescript": "^4.6.3"
    },
    "lint-staged": {
        "*.js": "eslint --cache --fix",
        "*.{js,css,md}": "prettier --write"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "husky": {
        "hooks": {
            "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
        }
    }
}